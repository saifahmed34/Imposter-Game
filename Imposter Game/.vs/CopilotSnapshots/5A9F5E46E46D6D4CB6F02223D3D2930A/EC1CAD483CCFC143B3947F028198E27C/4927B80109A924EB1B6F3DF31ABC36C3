using System;
using System.Collections.Generic;
using System.Text;

namespace ImposterGame.Domain.Entites
{
    public class Vote
    {
        public Guid VoterId { get; }
        public Guid TargetId { get; }

        public Vote(Guid voterId, Guid targetId)
        {
            VoterId = voterId;
            TargetId = targetId;
        }
    }

}
